<resources>

    <declare-styleable name="FlexibleButton">
        <attr name="fb_backgroundColor" format="color" />
        <attr name="fb_cornerRadius" format="dimension" />
        <attr name="fb_icon" format="reference" />
        <attr name="fb_iconSize" format="dimension" />
        <attr name="fb_iconPadding" format="dimension" />
        <attr name="fb_iconGravity">
            <enum name="start" value="0"/>
            <enum name="end" value="1"/>
            <enum name="top" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
    </declare-styleable>


</resources>
